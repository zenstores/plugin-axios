{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "targets": {
          "chrome": "58",
          "ie": "11"
        }
      }
      ]
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    [
      "transform-es2015-template-literals",
      {
        "loose": true,
        "spec": true
      }
    ]
  ]
}